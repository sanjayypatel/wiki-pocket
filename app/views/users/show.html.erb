<div class="col-md-8">
  <div class="tabbable tabs-left">
    <ul class="nav nav-tabs">
      <li class="active">
        <%= link_to "#{@user.username}'s Wikis", user_path(@user, anchor: 'my-wikis', tab: "wikis", page: 1), "data-toggle" => "tab" %>
      </li>
      <li>
        <%= link_to "Shared Wikis", user_path(@user, anchor: 'shared-wikis'), "data-toggle" => "tab", tab: "shared", page: 1 %>
      </li>
      <li>
        <%= link_to "#{@user.username}'s Links",user_path(@user, anchor: 'my-links'), "data-toggle" => "tab", tab: "links", page: 1 %>
      </li>
    </ul>
    <div class="tab-content">
      <div class="tab-pane active" id="my-wikis"><br>
        <% @wikis.each do |wiki| %>
          <%= render partial: 'wikis/excerpt', locals: {wiki: wiki} %>
        <% end %>
        <%= will_paginate @wikis %>
      </div>

      <div class="tab-pane" id="shared-wikis"><br>
        <% @shared_wikis.each do |wiki| %>
          <%= render partial: 'wikis/excerpt', locals: {wiki: wiki} %>
        <% end %>
        <%= will_paginate @shared_wikis %>
      </div>

      <div class="tab-pane" id="my-links"><br>
        <% @links.each do |link| %>
          <%= render partial: 'links/link', locals: {link: link} %>
        <% end %>
        <%= will_paginate @links %>
      </div>
    </div>
  </div>
</div>
<div class="col-md-4">
  <%= render partial: 'users/controls' %>
  <%= render partial: 'users/user', locals: {user: @user} %>
</div>
