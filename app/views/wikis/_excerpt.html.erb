<div class="panel panel-info">
  <div class="panel-heading clearfix">
    <h4 class="pull-left"><%= link_to wiki.title, wiki_path(wiki) %></h4>
    <%= render partial: 'wikis/controls', locals: {wiki: wiki} %>
  </div>
  <div class="panel-body">
    <p><%= truncate(wiki.body, length: 150) %> </p>
    <p>
      <% wiki.users.each do |user| %>
        <%= user.username %><br>
      <% end %>
    </p>
  </div>
</div>