<%= form_for wiki do |f| %>
  <div class="form-group pull-right">
    <%= f.submit "Save", class: 'btn btn-success' %>
  </div><br>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control', autofocus: true, autocomplete: false %>
  </div>
  <div class="form-group">
    <%= f.label :body %>
    <%= f.text_area :body, class: 'form-control', rows: 20, autofocus: true, autocomplete: false %>
  </div>
<% end %>